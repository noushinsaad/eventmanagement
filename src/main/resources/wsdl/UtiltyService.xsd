        elementFormDefault="qualified">

    <element name="Utilities" type="tns:Utilities"/>

    <complexType name="Utilities">
        <sequence>
            <element name="utility" type="xsd:string" maxOccurs="unbounded"/>
        </sequence>
    </complexType>
</schema>
